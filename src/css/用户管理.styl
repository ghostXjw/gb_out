border = 1px solid #eaeaea

.m-userprofile
  width 1200px
  margin 0 auto
  box-sizing border-box
  margin-bottom 10px
  padding-left 20px
  position relative
  >.line
    line-height 32px
    font-size 14px
    >*
      display inline-block
    .l
      color #333333
      width 84px
    .r
      color #22a4e0
      margin-right 20px
    .m
      font-size 12px
      color #999999
  >.addbtn
    width 111px
    height 42px
    border 1px solid #22a4e0
    border-radius 4px
    line-height @height
    text-align center
    position absolute
    right 0
    bottom 10px
    font-size 14px
    color #22a4e0
    cursor pointer
    .i
      margin-right 9px

.table
  border border
  border-radius 4px
  width 1200px
  margin 0 auto
  margin-bottom 40px
  .th
  .tr
    display table
    table-layout fixed
    border-bottom border
    width 100%
    font-size 14px
    color #666666
    &:last-child
      border-bottom 0
    >.item
      display table-cell
      border-right border
      vertical-align middle
      text-align center
      &:last-child
        border-right 0
      &.l0
        width 72px
      &.l1
        width 98px
      &.l2
        width 200px
      &.l3
        width 96px
      &.l4
        width 550px
      &.l6
        width 88px
      &.l6
      &.l7
        cursor pointer
  .th
    line-height 40px
  .tr
    line-height 24px
    >.item.l4
      padding 9px 0

.m-pagenum
  padding-bottom 16px
  text-align center

.m-fwin
  .m-selector
    width 200px
    float left
